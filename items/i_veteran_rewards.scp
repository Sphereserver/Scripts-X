   ENDIF

[TYPEDEF t_power_tool]
ON=@DropOn_Self
   IF (<USESCUR> >= <USESMAX>)
      SRC.SYSMESSAGE Adding this to the power tool would put it over the max number of charges the tool can hold.
   ELSE
      LOCAL.Charges=<eval <USESMAX> - <USESCUR>>
      IF (<LOCAL.Charges> > <ARGO.USESCUR>)
         LOCAL.Charges=<ARGO.USESCUR>
      ENDIF
      USESCUR += <LOCAL.Charges>
      ARGO.USESCUR -= <LOCAL.Charges>
      IF (<ARGO.USESCUR> < 1)
         ARGO.REMOVE
      ENDIF
      SOUND 042
      SRC.SYSMESSAGE Charges have been added to the power tool.
   ENDIF
   return 1

