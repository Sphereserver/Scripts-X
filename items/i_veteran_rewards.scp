[ITEMDEF 0a20a]
DEFNAME=i_embroidery_tool
TAG.EventName=e_embroidered
TAG.DialogTitleCliloc=1158846	//<CENTER>Embroidery Tool</CENTER>
TAG.DialogMsgSuccess="You embroider the object."
TAG.DialogMsgRemove="You remove the embroidery from the object."
TAG.DialogMsgFailed="The embroidery failed."
CATEGORY=Veteran Rewards
SUBSECTION=Year 4
DESCRIPTION=Embroidery Tool
ON=@Create
   ATTR=attr_newbie
   USESMAX=10
ON=@ClientTooltip
   SRC.ADDCLILOC 1076220	//4th Year Veteran Reward
ON=@DClick
   IF (<TOPOBJ> != <SRC>)
      SRC.SYSMESSAGE This item must be in your backpack to be used.
   ELIF (<USESCUR> <= 0)
      SRC.SYSMESSAGE There are no charges left on this embroidery tool.
   ELSE
      TARGET Select an object to embroider
   ENDIF
   return 1
ON=@TargOn_Item
   IF (<ARGO.TOPOBJ> != <SRC>)
      SRC.SYSMESSAGE The selected item is not accessible to engrave.
   ELIF (<ARGO.TYPE> != t_clothing)
      SRC.SYSMESSAGE The selected item cannot be embroidered by this tool.
   ELSE
      SRC.CTAG.EngravingTool=<UID>
      SRC.CTAG.EngravingItem=<ARGO>
      SRC.SYSMESSAGE @,,1 Type message to engrave.
      SRC.PROMPTCONSOLE f_EngravingTool
   ENDIF
   return 1
ON=@TargOn_Char
   SRC.SYSMESSAGE The selected item cannot be embroidered by this tool.
   return 1

[FUNCTION f_EngravingTool]
REF1=<CTAG0.EngravingTool>
REF2=<CTAG0.EngravingItem>
CTAG.EngravingTool=
CTAG.EngravingItem=
IF (<ISEMPTY <ARGS>>)
	SYSMESSAGE The name entered was empty.
	return 1
ELIF <EVAL STRLEN(<ARGS>)> > 20
  SYSMESSAGE That name is too long (20 chars max).
  return 1
ENDIF
REF2.EVENTS=+e_embroidered
REF2.TAG.Engraved=<args>
REF1.USESCUR -= 1
SYSMESSAGE @,,1 <REF2.NAME> has been embroidered with a new message.
REF2.RESENDTOOLTIP 1

[TYPEDEF e_embroidered]
ON=@ClientTooltip
   ADDCLILOC 1158847,<TAG.Engraved>	//Embroidered: ~1_MESSAGE~

